<%- include ("../partials/header") %>

  <div class="container-fluid">
    <section id="form">
      <form id="myForm" class="needs-validation" novalidate action="/faculty/fac_entrepreneurshipDetails">
        <div class="container-fluid">

          <!-- Header Section -->

          <div class="row data-row">
            <div class="col-md-8 mb-3">
              <h4 class="mb-3 title">Entrepreneurship Details</h4>
            </div>
            <div class="col-md-4 mb-3 report">
              <a href="/faculty/search" class="btn btn-sm btn-primary shadow-sm"><i
                  class="fas fa-download fa-sm text-white-50"></i> Generate Report</a>
            </div>
          </div>
          

          <div class="row data-row">

            <div class="col-md-6 mb-3">
              <label class="required">Company Name</label>
              <input type="text" name="CompanyName" class="form-control">
            </div>
            <div class="col-md-6 mb-3">
              <label class="required">Company Address</label>
              <input type="text" name="CompanyAddress" class="form-control">
            </div>
          </div>
          <div class="row data-row">

            <div class="col-md-6 mb-3">
              <label for="date" class="required">Establishment Date</label>
              <input type="date" name="establishmentDate" class="form-control" onchange="setDate()">
            </div>
            
          </div>
          <div class="row data-row">
            
            <div class="col-md-6 mb-3">
              <label>Revenue Generated</label>
              <input type="text" name="revenue" class="form-control">
            </div>
            
            
            <div class="col-md-6 mb-3">
              <label class="required">Objectives</label>
              <input type="text" name="objectives" class="form-control">
            </div>
            <div class="col-md-6 mb-3">
              <label class="required">Benefit in terms of learning/Skill/Knowledge obtained</label>
              <input type="text" name="benefit" class="form-control">
            </div>
            <div class="col-md-6 mb-3">
              <label class="required">Company Type</label>
              <input type="text" name="Companytype" class="form-control">
            </div>
            


          </div>
          <div class="row data-row">
            
            
            

            <div class="col-md-4 mb-3">
              <label class="required">COE</label>

              <!-- COE  -->

              <%- include ("../partials/coe") %>

                <div class="invalid-feedback">
                  Please select a valid type.
                </div>
            </div>
          </div>
          <label class="required">Description</label><br>

          <div name="description" id="editor">
            <p>This is the editor content.</p>
          </div>
          <div class="submit-button"><button class="btn btn-primary btn-lg btn-block buttons" type="submit">
              Submit</button>
          </div>
        </div>

      </form>



    </section>

  </div>

  <!-- Footer -->


  <%- include ("../partials/footer") %>

   <script  type="text/javascript" src="/js/date.js">
    </script>

    </body>

    </html>