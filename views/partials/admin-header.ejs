<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="index.html">
                <div class="sidebar-brand-icon">
                    <i class="fas fa-users-cog"></i>
                </div>
                <div class="sidebar-brand-text mx-3">Admin</div>
            </a>


            <!-- Nav Item - Dashboard -->
            <li <% if (title === 'Dashboard') { %> class="nav-item active" <% } else { %> class="nav-item" <% } %>>
                <a class="nav-link" href="/admin">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span></a>
            </li>

            <li <% if (title === 'Add User') { %> class="nav-item active" <% } else { %> class="nav-item" <% } %>>
                <a class="nav-link" href="/add-user">
                    <i class="fas fa-user-plus"></i>
                    <span>Add User</span></a>
            </li>

            <li <% if (title === 'roles') { %> class="nav-item active" <% } else { %> class="nav-item" <% } %>>
                <a class="nav-link" href="/roles">
                    <i class="fas fa-tasks"></i>
                    <span>Manage Roles</span></a>
            </li>
            <li <% if (title === 'remove-user') { %> class="nav-item active" <% } else { %> class="nav-item" <% } %>>
                <a class="nav-link" href="/remove-user">
                    <i class="fas fa-user-times"></i>
                    <span>Remove User</span></a>
            </li>



            <!-- Sidebar Toggler (Sidebar) -->
            <div class="text-center d-none d-md-inline sidebar-button">
                <button class="rounded-circle border-0" id="sidebarToggle"></button>
            </div>


        </ul>
